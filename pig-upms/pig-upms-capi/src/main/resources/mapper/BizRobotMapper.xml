<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pig4cloud.pig.capi.mapper.BizRobotMapper">

  <resultMap id="bizRobotMap" type="com.pig4cloud.pig.admin.api.entity.BizRobot">
    <id property="id" column="id"/>
    <result property="status" column="status"/>
    <result property="robotUrl" column="robot_url"/>
    <result property="host" column="host"/>
    <result property="robotProxies" column="robot_proxies"/>
    <result property="robotAccountPassword" column="robot_account_password"/>
    <result property="robotProxiesName" column="robot_proxies_name"/>
    <result property="needDynamicProxy" column="need_dynamic_proxy"/>
    <result property="testVin" column="test_vin"/>
    <result property="serviceStartTime" column="service_start_time"/>
    <result property="serviceEndTime" column="service_end_time"/>
    <result property="createBy" column="create_by"/>
    <result property="createTime" column="create_time"/>
    <result property="updateTime" column="update_time"/>
    <result property="updateBy" column="update_by"/>
  </resultMap>
</mapper>
